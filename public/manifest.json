{
    "manifest_version": 3,
    "name": "ViSync",
    "version": "0.01",
    "description": "A way to synchronize video streams",
    "background": {
        "service_worker": "bundler.js",
        "type": "module"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "./index.html"
    }
}